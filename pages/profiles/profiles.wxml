<!-- 个人主页页面结构 -->
<view class="userHeadPic">
  <image src="{{gender =='boy'?'/img/@2x.png':'/img/@2x.png'}}"></image>
  <view class="userName">{{userName}}</view>
</view>
<view class="vipTime">
  <view class="weui-cells weui-cells_after-title">
    <view class="weui-cell">
      <view class="weui-cell__bd">VIP会员到期时间</view>
      <view decode='{{true}}' class="weui-cell__ft disabled-time ">
        <text class="date">2019-12-25</text>
        <text class="time">12:36</text>
      </view>
    </view>
  </view>
</view>
<view class="control-info">
  <view class="weui-cells weui-cells_after-title">
    <navigator url="/pages/profiles/bindMobile/bindMobile" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">绑定手机号</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
    <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">我的关注</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
    <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">使用须知</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
    <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__bd">我的客服</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
  </view>
</view>